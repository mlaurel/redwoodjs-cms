datasource DS {
  provider = "sqlite"
  url = "***"
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = env("BINARY_TARGET")
}

// Define your own datamodels here and run `yarn redwood db save` to create
// migrations for them.
model Product {
  id                     Int     @default(autoincrement()) @id
  name               String
  price                Float
  description    String?
  image              String?
}
